<audit-report-config name="PRODUCT">
    <given-root-type>User</given-root-type>
    <types>
        <type code="User" displayName="User">
            <atomic-attributes>
                <atomic-attribute qualifier="name" displayName="name"/>
                <!-- Qualifier customerID exist in Customer type - and using this qualifier in User type is correct-->
                <atomic-attribute qualifier="customerID" displayName="customerID"/>
            </atomic-attributes>
        </type>
    </types>
</audit-report-config>